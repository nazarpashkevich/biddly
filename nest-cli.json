{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "src/modules",
  "compilerOptions": {
    "deleteOutDir": true,
    "assets": [
      {
        "include": "**/*.hbs",
        "outDir": "dist/modules",
        "watchAssets": true
      }
    ]
  },
  "entryFile": "main"
}
